package aula3;

public class ListaLigada {

	public static void main(String[] args) {
		
		System.out.println("Cada elemento sabe apenas quem é o seu próximo."
				+ "\nApenas 1 ponteiro para o próximo elemento."
				+ "\nMais fácil de adicionar e remover elementos."
				+ "\nPara encontar um elemento tem que percorrer a lista inteira."
				+ "\nNão tem como fazer acesso direto pelo index do elemento.");

	}

}
